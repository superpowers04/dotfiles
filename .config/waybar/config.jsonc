    },
    "custom/power": {
        "format" : " ‚èª ",
        "on-click":"nwg-bar"
		// "tooltip": false,
    },
    "custom/close": {
        "format" : "|X|",
        "on-click":"cwctl -c 'cwc.client.focused():close()'"
		// "tooltip": false,
    },
    "custom/maximize": {
        "format" : "[=]",
        "on-click":"cwctl -c 'cwc.client.focused():maximize()'"
		// "tooltip": false,
    },
    "custom/floating": {
        "format" : "|^|",
        "on-click":"cwctl -c 'local c = cwc.client.focused(); c.floating = not c.floating'"
		// "tooltip": false,
    },
    "custom/swaync": {
	    "tooltip": false,
	    "format": "{icon}",
	    "format-icons": {
	      "notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span> ",
	      "none": "ÔÇ¢",
	      "dnd-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span> ",
	      "dnd-none": "Ôá∑",
	      "inhibited-notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span> ",
	      "inhibited-none": "ÔÇ¢",
	      "dnd-inhibited-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span> ",
	      "dnd-inhibited-none": "Ôá∑"
	    },
	    "return-type": "json",
	    "exec-if": "which swaync-client",
	    "exec": "swaync-client -swb",
	    "on-click": "swaync-client -t -swb",
	    "on-click-right": "swaync-client -d -swb",
	    "escape": true
    },
    "hyprland/workspaces": {
